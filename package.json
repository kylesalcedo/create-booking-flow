        "nanoid": "3.x",
        "next": "^14.0.0",
        "ora": "^5.4.1",
        "polished": "4.x",
        "react": "^18.0.0",
        "react-day-picker": "^9.0.0",
        "react-device-detect": "^1.17.0",
        "react-dom": "^18.0.0",
        "react-icons": "^4.2.0",
        "react-map-gl": "^6.1.17",
        "react-spinners": "^0.11.0",
        "react-spring": "^9.3.2",
