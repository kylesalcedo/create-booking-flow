        "@mui/icons-material": "^5.15.0",
        "@mui/lab": "^5.0.0-alpha.170",
        "@mui/material": "^5.15.0",
        "@mui/styles": "^5.15.0",
        "@mui/x-date-pickers": "^7.0.0",
        "@react-google-maps/api": "^2.19.0",
        "aws-sdk": "^2.968.0",
        "axios": "0.21.1",
        "bootstrap": "^5.1.0",
        "card-validator": "^8.1.1",
