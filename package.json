        "@types/glob": "^7.2.0",
        "@types/gtag.js": "^0.0.8",
        "@types/haversine": "^1.1.5",
        "@types/mapbox-gl": "^2.4.2",
        "@types/node": "^16.4.12",
        "@types/react": "^18.0.0",
        "@types/styled-components": "5.x",
        "babel-loader": "8.x",
        "concurrently": "6.x",
        "eslint": "^7.29.0",
        "eslint-config-next": "^14.0.0",
        "husky": "^7.0.0",
        "prettier": "2.x",
